{
  "name": "brand-kit-backend",
  "version": "1.0.0",
  "description": "Backend API for Brand Kit subscription platform",
  "main": "src/server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "db:setup": "psql -U postgres -f src/database/schema.sql",
    "db:seed": "psql -U postgres -d brandkit -f src/database/seeds.sql",
    "db:reset": "npm run db:setup && npm run db:seed"
  },
  "keywords": ["brand", "subscription", "ecommerce", "api"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "joi": "^17.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
